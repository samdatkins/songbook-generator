<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Live Playlist - All Power Hours</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.7.2/css/bulma.min.css"
    />
    <script
      defer
      src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"
    ></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="/scripts/livePlaylistNav.js"></script>
    <script src="/scripts/jquery.qrcode.min.js"></script>
    <link rel="stylesheet" href="/style/livePlaylist.css" />
  </head>

  <body>
    <section class="section">
      <ul>
        <% for(var i=0; i < songbookList.length; i++) { %>
        <li>
          <%= songbookList[i].title %> (<a
            href="/live/<%= songbookList[i].session_key %>/view"
            ><%= songbookList[i].session_key %></a
          >) -
        </li>
        <% } %>
      </ul>
    </section>
  </body>
</html>
